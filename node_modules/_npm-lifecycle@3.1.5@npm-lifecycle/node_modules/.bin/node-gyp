#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_node-gyp@5.1.1@node-gyp/bin/node-gyp.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_node-gyp@5.1.1@node-gyp/bin/node-gyp.js" "$@"
  ret=$?
fi
exit $ret
